{
    "name": "angular-three",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "packages/angular-three/src",
    "prefix": "angular-three",
    "targets": {
        "build": {
            "executor": "@nrwl/angular:package",
            "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
            "options": {
                "project": "packages/angular-three/ng-package.json"
            },
            "configurations": {
                "production": {
                    "tsConfig": "packages/angular-three/tsconfig.lib.prod.json"
                },
                "development": {
                    "tsConfig": "packages/angular-three/tsconfig.lib.json"
                }
            },
            "defaultConfiguration": "production"
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "packages/angular-three/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "packages/angular-three/**/*.ts",
                    "packages/angular-three/**/*.html",
                    "packages/angular-three/objects/**/*.ts",
                    "packages/angular-three/objects/**/*.html",
                    "packages/angular-three/attributes/**/*.ts",
                    "packages/angular-three/attributes/**/*.html",
                    "packages/angular-three/geometries/**/*.ts",
                    "packages/angular-three/geometries/**/*.html",
                    "packages/angular-three/materials/**/*.ts",
                    "packages/angular-three/materials/**/*.html",
                    "packages/angular-three/audios/**/*.ts",
                    "packages/angular-three/audios/**/*.html",
                    "packages/angular-three/cameras/**/*.ts",
                    "packages/angular-three/cameras/**/*.html",
                    "packages/angular-three/helpers/**/*.ts",
                    "packages/angular-three/helpers/**/*.html",
                    "packages/angular-three/lights/**/*.ts",
                    "packages/angular-three/lights/**/*.html"
                ]
            }
        }
    },
    "tags": []
}
