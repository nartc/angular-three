{
    "name": "soba",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "packages/soba/src",
    "prefix": "drei",
    "targets": {
        "build": {
            "executor": "@nrwl/angular:package",
            "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
            "options": {
                "project": "packages/soba/ng-package.json"
            },
            "configurations": {
                "production": {
                    "tsConfig": "packages/soba/tsconfig.lib.prod.json"
                },
                "development": {
                    "tsConfig": "packages/soba/tsconfig.lib.json"
                }
            },
            "defaultConfiguration": "production"
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "packages/soba/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "packages/soba/**/*.ts",
                    "packages/soba/**/*.html",
                    "packages/soba/controls/**/*.ts",
                    "packages/soba/controls/**/*.html"
                ]
            }
        }
    },
    "tags": []
}
